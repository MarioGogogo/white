<!DOCTYPE html>
<html lang="en">
<%- partial('_partial/head') %>

<body>
    <div id="main">
        <!-- 头部导航 -->
        <%- partial('_partial/nav') %>
        <!-- 引入正文 -->
        <div id="content">
            <%- body %>
        </div>
    </div>
    <!-- footer -->
    <%- partial('_partial/footer') %>
      <!-- 返回顶部 -->
    <%- partial('_partial/totop') %>
     <!-- 全局搜索框 -->
    <% if(theme.local_search.enable) { %>
            <%- partial('_partial/search') %>
      <% } %>
    <!-- 引入 jquery -->
    <%- js('/lib/jquery3.4.1.js') %>
    <!-- 顶部加载条 -->
    <%- js('js/nprogress.js') %>
    <!-- 引入 js 文件 -->
    <%- js('js/main.js') %>
    <!-- 引入代码高亮的 js -->
    <%- js('/lib/highlight/highlight.pack.js') %>
    <!-- 评论js -->
    <%- js('/js/Valine.min.js') %>
    <!-- 引入 搜索js 文件 -->
    <%- js('js/search2.js') %>
    <script>
      hljs.initHighlightingOnLoad()
    </script>
</body>

</html>